# Mobile iot config
10086.iot.v1910.apiKey=4DBRwHN2bO=Gg0gnN=R1UC=ZdIs=
10086.iot.v1910.objId=3200
10086.iot.v1910.objInstId=0
10086.iot.v1910.readResId=5750
10086.iot.v1910.expireOffset=360000
10086.iot.v1910.startChar=68
10086.iot.v1910.endChar=16
10086.iot.v1910.meterType=10
10086.iot.v1910.writeMode=1
# Telecom platform iot config
telecom.iot.manufacturerId=Wasion
telecom.iot.manufacturerName=Wasion
telecom.iot.deviceType=WaterMeter
telecom.iot.model=NbWater_NBIotFff
telecom.iot.protocolType=CoAP

task.iot.upload.recorder.enable=true
task.iot.command.process.enable=true
ipaccess.limit.enable=true

spring.profiles.active=pro,resolverCfg
# meter reading
server.servlet.context-path=/mr

logging.level.root=INFO
logging.level.com.wasion.meterreading=INFO
logging.level.org.springframework.boot=INFO
logging.pattern.console=%clr(%d{yyyy-MM-dd-HH:mm}){faint} [%thread] %clr(%-5level){cyan} %logger- %msg%n 
logging.pattern.file=%d{yyyy-MM-dd HH:mm} [%thread] %-5level %logger- %msg%n
logging.file=D:/mrlog/springbootlog.log

spring.jackson.date-format=yyyy-MM-dd HH:mm:ss
spring.jackson.time-zone=GMT+8

# Redis数据库索引（默认为0）
spring.redis.database=0  
# Redis服务器地址
spring.redis.host=localhost
# Redis服务器连接端口
spring.redis.port=6379  
# Redis服务器连接密码（默认为空）
spring.redis.password=
# 连接池最大连接数（使用负值表示没有限制）
spring.redis.jedis.pool.max-active=200  
# 连接池最大阻塞等待时间（使用负值表示没有限制）
spring.redis.jedis.pool.max-wait=30000
# 连接池中的最大空闲连接
spring.redis.jedis.pool.max-idle=10 
# 连接池中的最小空闲连接
spring.redis.jedis.pool.min-idle=0  
# 连接超时时间（毫秒）
spring.redis.timeout=3000