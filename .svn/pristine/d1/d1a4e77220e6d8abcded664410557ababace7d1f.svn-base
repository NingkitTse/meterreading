package com.wasion.meterreading.domain.dto.telecom;

import java.util.List;

import com.wasion.meterreading.domain.BaseValue;

public class TelecomRequestBody extends BaseValue {

	/**
	 * 
	 */
	private static final long serialVersionUID = -9101127942892158559L;
	private String notifyType;
	private String requestId;
	private String deviceId;
	private String gatewayId;
	private List<ServiceData> services;

	public String getNotifyType() {
		return notifyType;
	}

	public void setNotifyType(String notifyType) {
		this.notifyType = notifyType;
	}

	public String getRequestId() {
		return requestId;
	}

	public void setRequestId(String requestId) {
		this.requestId = requestId;
	}

	public String getDeviceId() {
		return deviceId;
	}

	public void setDeviceId(String deviceId) {
		this.deviceId = deviceId;
	}

	public List<ServiceData> getServices() {
		return services;
	}

	public void setServices(List<ServiceData> services) {
		this.services = services;
	}

	public String getGatewayId() {
		return gatewayId;
	}

	public void setGatewayId(String gatewayId) {
		this.gatewayId = gatewayId;
	}

}
